@using TemplateParser = K9.SharedLibrary.Helpers.TemplateParser
@{
    ViewBag.Title = Dictionary.FAQTitle;
    ViewBag.SubTitle = Dictionary.FAQTitle;

    Layout = "../Shared/_LayoutForm.cshtml";
}

@Html.Partial("_BetaPopup")

<div class="container main-content privacy-policy">
    <article>
        @using (Html.BeginBootstrapForm())
        {
            @MvcHtmlString.Create(TemplateParser.Parse(Dictionary.faq, new
            {
                CalculationMethodsLink = Url.Action("CalculationMethods")
            }))
        }
    </article>
</div>

