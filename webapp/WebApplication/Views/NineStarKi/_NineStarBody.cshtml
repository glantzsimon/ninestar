@using K9.WebApplication.Helpers
@using K9.WebApplication.Models
@model NineStarKiEnergy

    @if (Model != null)
    {
        <div class="container main-content clear-both ninestar-header-container">
            <article>
                <div>
                    @Html.CollapsiblePanel(Model.FullEnergyDetailsTitle, Model.EnergyDescription, true)

                    @Html.CollapsiblePanel(Html.GetDisplayNameFor(m => Model.Trigram), Model.TrigramDescription, false, "", Model.TrigramUIName)
                </div>
            </article>
        </div>
    }