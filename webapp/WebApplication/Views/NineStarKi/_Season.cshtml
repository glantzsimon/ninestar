@using K9.WebApplication.Config
@model K9.WebApplication.Models.NineStarKiEnergy

<div class="ninestar-summary-container">
    <h3 class="margin-bottom-0 padding-bottom-0">@Dictionary.Season</h3>
    <p>@Model.SeasonDescription</p>
    <img loading="lazy" title="@Model.SeasonDescription" alt="@Model.SeasonDescription" class="auto-size center-block" src="@DefaultValuesConfiguration.Instance.BaseImagesPath/ninestar/seasons/@(Model.Season.ToLower()).png" />
</div>