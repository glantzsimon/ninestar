@using K9.SharedLibrary.Helpers
@using K9.WebApplication.Helpers

@{
    ViewBag.Title = Dictionary.RelationshipsTitle;
    ViewBag.SubTitle = Dictionary.RelationshipsTitle;
    ViewBag.Keywords = Dictionary.RelationshipsMetaDescription;
    ViewBag.MetaDescription = Dictionary.RelationshipsMetaDescription;

    Layout = "../Shared/_LayoutForm.cshtml";
}

@Html.Partial("_BetaPopup")

<div class="container main-content">
    <article>
        @Html.Panel("9 Star Ki and Relationships", TemplateProcessor.PopulateTemplate(Dictionary.relationships_description, new
   {
       CompatibilityLink = Url.Action("Compatibility", "NineStarKi"),
   }), "relationships")

        @Html.Panel("The 5 Transformations", Html.Partial("_FiveTransformations").ToString(), "transformations")

        @Html.Panel("9 Star Ki and Sexuality", TemplateProcessor.PopulateTemplate(Dictionary.sexuality, new
        {
            AboutTitle = Dictionary.AboutNineStarKi,
            AboutLink = Url.ActionWithBookmark("About", "Home", "yinyang"),
            ChartLink = Url.ActionWithBookmark("About", "Home", "chart"),
        }), "sexuality")
    </article>

    <br />

    @Html.BootstrapActionLinkButton(Dictionary.RelationshipCompatibilityCalculator, "Compatibility", "NineStarKi", null, "fa fa-heart")
</div>

