@model K9.WebApplication.Models.NineStarKiModel
<div class="inline-form">
    @using (Html.BeginForm("IndexPost", "PersonalChart", FormMethod.Post))
    {
        @Html.AntiForgeryToken()
        @Html.HiddenFor(model => model.PersonModel.Name)
        @Html.HiddenFor(model => model.PersonModel.DateOfBirth)
        @Html.HiddenFor(model => model.PersonModel.TimeOfBirth)
        @Html.HiddenFor(model => model.PersonModel.BirthTimeZoneId)
        @Html.HiddenFor(model => model.PersonModel.Gender)
        @Html.HiddenFor(model => model.SelectedDate)
        @Html.HiddenFor(model => model.SelectedTime)
        @Html.HiddenFor(model => model.TimeZoneId)

        @Html.BootstrapButton(Dictionary.PersonalProfileCalculator, EButtonType.Submit, "fa-yin-yang", EButtonClass.Large)
    }
</div>