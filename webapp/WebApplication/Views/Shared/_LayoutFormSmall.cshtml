@{
	Layout = "~/Views/Shared/_LayoutMaster.cshtml";
}
@section layout
{
	<div class="mainContent">
		<div class="backdrop">
			<div class="page shadow alphaBackground roundCorners small-form">
				<div class="page-header">
					<h4>@ViewBag.SubTitle</h4>
				</div>
				@RenderBody()
			</div>
		</div>
	</div>
}