@model string

@{
    var description = ViewBag.MetaDescription ?? K9.Globalisation.Dictionary.NineStarMetaDescriptionMain;
    var keywords = ViewBag.Keywords ?? K9.Globalisation.Dictionary.Keywords;
    var title = Html.Raw($"{ViewBag.Title} &raquo; {K9.Globalisation.Dictionary.NineStar} &raquo; {keywords}");
}

<div class="seo @Model">
    @for (int i = 0; i < 3; i++)
    {
        <h1>
            @title
            @description
        </h1>
        <div>
            @Html.Raw(K9.Globalisation.Dictionary.origins)
        </div>
        <h2>
            @title
            @keywords
        </h2>
        <h3>
            @title
            @keywords
        </h3>
        <h4>
            @title
            @keywords
        </h4>
    }
</div>