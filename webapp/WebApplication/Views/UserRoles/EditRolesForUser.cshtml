@model K9.WebApplication.ViewModels.UserRolesViewModel
@{
	Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
@using (Html.BeginForm())
{
	using (Html.BeginBootstrapForm())
	{
		foreach (var item in Model.UserRoles)
		{
			@Html.BootstrapDisplayFor(model => item.Role.Description)
			@Html.BootstrapEditorFor(model => item.IsSelected)
		}
		
	<hr />
	@Html.BootstrapButton(Dictionary.Update)
	@Html.BootstrapBackToListButton()
	}
}