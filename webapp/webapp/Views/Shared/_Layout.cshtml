<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1" />
	<title>@ViewBag.Title - @Dictionary.NineStarKiTitle</title>
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
	<link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/v/bs/jszip-3.1.3/pdfmake-0.1.27/dt-1.10.15/b-1.3.1/b-colvis-1.3.1/b-html5-1.3.1/b-print-1.3.1/cr-1.3.3/r-2.1.1/se-1.2.2/datatables.min.css"/>
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.0/jquery.min.js"></script>
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
	<script type="text/javascript" src="https://cdn.datatables.net/v/bs/jszip-3.1.3/pdfmake-0.1.27/dt-1.10.15/b-1.3.1/b-colvis-1.3.1/b-html5-1.3.1/b-print-1.3.1/cr-1.3.3/r-2.1.1/se-1.2.2/datatables.min.js"></script>
	@Styles.Render("~/Content/css")
</head>
<body>
<nav class="navbar navbar-default">
	@Html.Partial("_Navbar")
</nav>
<div class="container container-fluid">
	<div class="page-header">
		<h1>@ViewBag.Title</h1>
	</div>
	<div class="container text-center">
		<div class="row">
			<div class="col-sm-3 well">
				@RenderSection("left")
			</div>
			<div class="col-sm-7">
				@RenderSection("middle")
			</div>
			<div class="col-sm-2 well">
				@RenderSection("right")
			</div>
		</div>
	</div>
	@RenderBody()
</div>
<footer class="container-fluid text-center">
	<p>@Dictionary.NineStarKiTitle</p>
</footer>
@RenderSection("scripts", required: false)
</body>
</html>